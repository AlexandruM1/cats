<div ng-app="TRNCON">
    <table class="table">
        <tr>
            <th>Contract Number</th>
            <th>Progress</th>
        </tr>
        <tr ng-repeat="sd in contracts">
            <td>{{sd.ContractNumber}}</td>
            <td>
                <div class="progress progress-success">
                    <div class="bar" style="width: 60%">
                        <span class="sr-only">60% Completeted</span>
                    </div>
                </div>
                <div>
                    Days before contract ends: {{sd.days}} 
                </div>
                <div>
                    Last Dispatched on: {{sd.lastdate}}
                </div>
            </td>
        </tr>
        <tr ng-repeat="sd in contracts">
            <td>{{sd.ContractNumber}}</td>
            <td>
                <div class="progress progress-success">
                    <div class="bar" style="width: 20%">
                        <span class="sr-only">20% Completeted</span>
                    </div>
                </div>
            </td>

        </tr>
        <tr ng-repeat="sd in contracts">
            <td>{{sd.ContractNumber}}</td>
            <td>
                <div class="progress progress-success">
                    <div class="bar" style="width: 75%">
                        <span class="sr-only">75% Completeted</span>
                        @*<span class="sr-only">50% Remaining</span>>*@
                    </div>
                </div>
            </td>

        </tr>
    </table>
</div>

