@using Cats.Helpers
@model  Cats.Models.ViewModels.HRD.DurationViewModel
@{
    ViewBag.Title = "DateInterval";
    const string PAGE_NAME = "EarlyWarning.HRD.DateInterval";
}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">@Html.Translate2(PAGE_NAME,"Date Interval")</h3>
</div>
<table border="0" class="form-horizontal">
        <tr >
            <td >
                <div class="control-label">
                    @Html.Label(@Html.Translate2(PAGE_NAME,"From(EC)"))
                </div>
                <div class="controls">
                    @Html.DisplayFor(m=>m.StartDateEC)
                </div>
            </td>
           
             <td >
                 <div class="control-label">
                    @Html.Label(@Html.Translate2(PAGE_NAME,"To(EC)"))
                </div>
                <div class="controls">
                    @Html.DisplayFor(m=>m.EndDateEC)
                </div>
                
            </td>
        </tr>
        <tr >
           
            <td >
                <div class="control-label">
                    @Html.Label(@Html.Translate2(PAGE_NAME,"From(GC)"))
                </div>
                <div class="controls">
                    @Html.DisplayFor(m=>m.StartDateGC)
                </div>
            </td>
           
             <td >
                 <div class="control-label">
                    @Html.Label(@Html.Translate2(PAGE_NAME,"To(GC)"))
                </div>
                <div class="controls">
                    @Html.DisplayFor(m=>m.EndDateGC)
                </div>
                
            </td>
        </tr>
            
    </table>
    
<div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
       
</div>